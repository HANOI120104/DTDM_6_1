[36m@@ -0,0 +1,42 @@[m
[32m+[m[32mimport os[m
[32m+[m[32mfrom google.cloud import storage, vision, firestore[m
[32m+[m
[32m+[m[32mdef process_image(event, context):[m
[32m+[m[32m    bucket_name = event['face-attendance-images'][m
[32m+[m[32m    file_name = event['name'][m
[32m+[m
[32m+[m[32m    print(f"New image uploaded: {file_name} in bucket {bucket_name}")[m
[32m+[m
[32m+[m[32m    # Init Cloud Vision client[m
[32m+[m[32m    vision_client = vision.ImageAnnotatorClient()[m
[32m+[m
[32m+[m[32m    # Init Cloud Storage client[m
[32m+[m[32m    storage_client = storage.Client()[m
[32m+[m[32m    bucket = storage_client.bucket(bucket_name)[m
[32m+[m[32m    blob = bucket.blob(file_name)[m
[32m+[m
[32m+[m[32m    image_uri = f"gs://{bucket_name}/{file_name}"[m
[32m+[m[32m    image = vision.Image(source=vision.ImageSource(image_uri=image_uri))[m
[32m+[m
[32m+[m[32m    # Call face detection[m
[32m+[m[32m    response = vision_client.face_detection(image=image)[m
[32m+[m[32m    faces = response.face_annotations[m
[32m+[m
[32m+[m[32m    print(f"Detected {len(faces)} face(s) in {file_name}")[m
[32m+[m
[32m+[m[32m    # Init Firestore client[m
[32m+[m[32m    db = firestore.Client()[m
[32m+[m
[32m+[m[32m    # Save attendance log (sample)[m
[32m+[m[32m    attendance_ref = db.collection('attendance_logs').document()[m
[32m+[m[32m    attendance_ref.set({[m
[32m+[m[32m        'file': file_name,[m
[32m+[m[32m        'bucket': bucket_name,[m
[32m+[m[32m        'face_count': len(faces),[m
[32m+[m[32m        'timestamp': context.timestamp,[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    print("Attendance logged.")[m
[32m+[m
[32m+[m[32m    if response.error.message:[m
[32m+[m[32m        raise Exception(f"Vision API Error: {response.error.message}")[m
